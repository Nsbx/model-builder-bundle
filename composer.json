{
    "name": "nsbx/model-builder-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Bondoux"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "symfony/console": "6.2.*",
        "symfony/framework-bundle": "6.2.*",
        "symfony/property-access": "^6.2"
    },
    "require-dev": {
        "symfony/test-pack": "^1.1",
        "symfony/yaml": "6.2.*"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Nsbx\\Bundle\\ModelBuilder\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Nsbx\\Bundle\\ModelBuilder\\Tests\\": "tests/" }
    }
}
